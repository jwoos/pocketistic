{
	"env": {
		"es6": true,
		"node": true,
		"jasmine": true,
		"jquery": true,
		"browser": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"always"
		],
		"no-console": [
			"warn",
			{
				"allow": ["warn", "error"]
			}
		],
		"no-unused-vars": ["warn"]
	},
	"globals": {
		"Trianglify": false,
		"swal": false
	}
}

